void whiskeyAndCoke() {
  pourWhiskey(1);
  pourCoke(2);
}

void mojito() {
  pourRum(1);
  pourSoda(2);
  pourCoke(1);
  pourWhiskey(1);
}

void rumAndCoke() {
  pourSyrup(1);
  pourCoke(2);
}

void snakebite() {
  pourLimeJuice(1.5);
  pourSoda(0.5);
}

void mapleNutShot() {
  pourWhiskey(2);
  pourSoda(1);
}

void sweetDaquiri() {
  pourLimeJuice(2);
  pourCoke(0.75);
  pourSoda(0.75);
}

