void whiskeyAndCoke() {
  pourWhiskey(1);
  pourCoke(2);
}

void mojito() {
  pourRum(2);
  pourSoda(2);
  pourSyrup(0.5);
  pourLimeJuice(0.5);
}

void rumAndCoke() {
  pourRum(1);
  pourCoke(2);
}

void snakebite() {
  pourWhiskey(1.5);
  pourLimeJuice(0.5);
}

void mapleNutShot() {
  pourWhiskey(2);
  pourRum(1);
}

void sweetDaquiri() {
  pourRum(2);
  pourSyrup(0.75);
  pourLimeJuice(0.75);
}

